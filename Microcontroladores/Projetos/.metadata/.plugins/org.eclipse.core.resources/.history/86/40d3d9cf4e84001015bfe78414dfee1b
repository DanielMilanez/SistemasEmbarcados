#ifndef INC_MCU6050_H_
#define INC_MCU6050_H_

#include "main.h"
#define MCU6050_ADDR (0x68 << 1)

// Registers
#define FS_ACC_2G 0
#define FS_ACC_4G 8

#define REG_CONFIG_GYRO 0x27
#define REG_CONFIG_ACC  0x28
#define REG_USR_CTRL	0x107
#define REG_DATA		0x59

HAL_StatusTypeDef MCU6050_StartUp(I2C_HandleTypeDef *hi2c);

#endif /* INC_MCU6050_H_ */
