/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{

  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  /* USER CODE BEGIN 2 */

  glcd_init();
  glcd_drawImage(loadingImage_bgBlack);
  glcd_loadingBar();
  glcd_drawImage(titleImage_bgBlack);

  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
  {
    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */
  }
  /* USER CODE END 3 */
}

/**
  * @brief System Clock Configuration
  * @retval None
  */
void SystemClock_Config(void)
{
  RCC_OscInitTypeDef RCC_OscInitStruct = {0};
  RCC_ClkInitTypeDef RCC_ClkInitStruct = {0};

  /** Configure the main internal regulator output voltage
  */
  __HAL_PWR_VOLTAGESCALING_CONFIG(PWR_REGULATOR_VOLTAGE_SCALE1);

  /** Initializes the RCC Oscillators according to the specified parameters
  * in the RCC_OscInitTypeDef structure.
  */
  RCC_OscInitStruct.OscillatorType = RCC_OSCILLATORTYPE_HSI;
  RCC_OscInitStruct.HSIState = RCC_HSI_ON;
  RCC_OscInitStruct.HSICalibrationValue = RCC_HSICALIBRATION_DEFAULT;
  RCC_OscInitStruct.PLL.PLLState = RCC_PLL_NONE;
  if (HAL_RCC_OscConfig(&RCC_OscInitStruct) != HAL_OK)
  {
    Error_Handler();
  }

  /** Initializes the CPU, AHB and APB buses clocks
  */
  RCC_ClkInitStruct.ClockType = RCC_CLOCKTYPE_HCLK|RCC_CLOCKTYPE_SYSCLK
                              |RCC_CLOCKTYPE_PCLK1|RCC_CLOCKTYPE_PCLK2;
  RCC_ClkInitStruct.SYSCLKSource = RCC_SYSCLKSOURCE_HSI;
  RCC_ClkInitStruct.AHBCLKDivider = RCC_SYSCLK_DIV2;
  RCC_ClkInitStruct.APB1CLKDivider = RCC_HCLK_DIV1;
  RCC_ClkInitStruct.APB2CLKDivider = RCC_HCLK_DIV1;

  if (HAL_RCC_ClockConfig(&RCC_ClkInitStruct, FLASH_LATENCY_0) != HAL_OK)
  {
    Error_Handler();
  }
}

/**
  * @brief GPIO Initialization Function
  * @param None
  * @retval None
  */
static void MX_GPIO_Init(void)
{
  GPIO_InitTypeDef GPIO_InitStruct = {0};
/* USER CODE BEGIN MX_GPIO_Init_1 */
/* USER CODE END MX_GPIO_Init_1 */

  /* GPIO Ports Clock Enable */
  __HAL_RCC_GPIOA_CLK_ENABLE();
  __HAL_RCC_GPIOB_CLK_ENABLE();
  __HAL_RCC_GPIOC_CLK_ENABLE();

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(GPIOA, EN_Pin|D0_Pin|D1_Pin|D5_Pin
                          |D4_Pin|RST_Pin, GPIO_PIN_RESET);

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(GPIOB, D6_Pin|CS2_Pin|D7_Pin|CS1_Pin
                          |D2_Pin|RS_Pin|RW_Pin, GPIO_PIN_RESET);

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(D3_GPIO_Port, D3_Pin, GPIO_PIN_RESET);

  /*Configure GPIO pins : EN_Pin D0_Pin D1_Pin D5_Pin
                           D4_Pin RST_Pin */
  GPIO_InitStruct.Pin = EN_Pin|D0_Pin|D1_Pin|D5_Pin
                          |D4_Pin|RST_Pin;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(GPIOA, &GPIO_InitStruct);

  /*Configure GPIO pins : D6_Pin CS2_Pin D7_Pin CS1_Pin
                           D2_Pin RS_Pin RW_Pin */
  GPIO_InitStruct.Pin = D6_Pin|CS2_Pin|D7_Pin|CS1_Pin
                          |D2_Pin|RS_Pin|RW_Pin;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(GPIOB, &GPIO_InitStruct);

  /*Configure GPIO pin : D3_Pin */
  GPIO_InitStruct.Pin = D3_Pin;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(D3_GPIO_Port, &GPIO_InitStruct);

/* USER CODE BEGIN MX_GPIO_Init_2 */
/* USER CODE END MX_GPIO_Init_2 */
}

/* USER CODE BEGIN 4 */
void glcd_init(void) {
	HAL_GPIO_WritePin(RST_GPIO_Port, RST_Pin, SET);
	HAL_GPIO_WritePin(CS1_GPIO_Port, CS1_Pin, RESET);
	HAL_GPIO_WritePin(CS2_GPIO_Port, CS2_Pin, RESET);
	HAL_GPIO_WritePin(EN_GPIO_Port, EN_Pin, RESET);
	HAL_GPIO_WritePin(RS_GPIO_Port, RS_Pin, RESET);
	HAL_GPIO_WritePin(RW_GPIO_Port, RW_Pin, RESET);

	glcd_writeByte(PAGE0, INSTRUCTION, 0xc0);
	glcd_writeByte(PAGE1, INSTRUCTION, 0xc0);
	glcd_writeByte(PAGE0, INSTRUCTION, 0x3e);
	glcd_writeByte(PAGE1, INSTRUCTION, 0x3e);
}

void glcd_writeByte(uint8_t chip, uint8_t rs, uint8_t data) {

	if(chip == PAGE0) {
		HAL_GPIO_WritePin(CS1_GPIO_Port, CS1_Pin, SET);
	} else {
		HAL_GPIO_WritePin(CS2_GPIO_Port, CS2_Pin, SET);
	}

	HAL_GPIO_WritePin(RS_GPIO_Port, RS_Pin, (rs == DATA ? SET:RESET));
	HAL_GPIO_WritePin(RW_GPIO_Port, RW_Pin, RESET);

	HAL_GPIO_WritePin(DB0_GPIO_Port, DB0_Pin, ((data & 0x01) == 0x01 ? SET:RESET));
	HAL_GPIO_WritePin(DB1_GPIO_Port, DB1_Pin, ((data & 0x02) == 0x02 ? SET:RESET));
	HAL_GPIO_WritePin(DB2_GPIO_Port, DB2_Pin, ((data & 0x04) == 0x04 ? SET:RESET));
	HAL_GPIO_WritePin(DB3_GPIO_Port, DB3_Pin, ((data & 0x08) == 0x08 ? SET:RESET));
	HAL_GPIO_WritePin(DB4_GPIO_Port, DB4_Pin, ((data & 0x10) == 0x10 ? SET:RESET));
	HAL_GPIO_WritePin(DB5_GPIO_Port, DB5_Pin, ((data & 0x20) == 0x20 ? SET:RESET));
	HAL_GPIO_WritePin(DB6_GPIO_Port, DB6_Pin, ((data & 0x40) == 0x40 ? SET:RESET));
	HAL_GPIO_WritePin(DB7_GPIO_Port, DB7_Pin, ((data & 0x80) == 0x80 ? SET:RESET));

	HAL_GPIO_WritePin(LCD_E_GPIO_Port, EN_Pin, SET);
	HAL_GPIO_WritePin(LCD_E_GPIO_Port, EN_Pin, RESET);

	HAL_GPIO_WritePin(CS1_GPIO_Port, CS1_Pin, RESET);
	HAL_GPIO_WritePin(CS2_GPIO_Port, CS2_Pin, RESET);
}

void glcd_displayStatus(uint8_t status) {
	if(status == OFF) {
		glcd_writeByte(PAGE0, INSTRUCTION, 0x3e);
		glcd_writeByte(PAGE1, INSTRUCTION, 0x3e);
	} else if (status == ON) {
		glcd_writeByte(PAGE0, INSTRUCTION, 0x3f);
		glcd_writeByte(PAGE1, INSTRUCTION, 0x3f);
	}
}

void glcd_fillScreen(uint8_t pixelStatus) {
	uint8_t i, j;
	glcd_displayStatus(OFF);
	for(j = 0; j < 8; j++) {
		glcd_writeByte(PAGE0, INSTRUCTION, 0x40);
		glcd_writeByte(PAGE1, INSTRUCTION, 0x40);
		glcd_writeByte(PAGE0, INSTRUCTION, 0xb8 + j);
		glcd_writeByte(PAGE1, INSTRUCTION, 0xb8 + j);
		for(i = 0; i < 64; i++) {
			glcd_writeByte(PAGE0, DATA, 0xff * pixelStatus);
			glcd_writeByte(PAGE1, DATA, 0xff * pixelStatus);
		}
	}
	glcd_displayStatus(ON);
}

void glcd_drawImage(uint8_t imageArr[8][128]) {
	uint8_t i, j;
	glcd_displayStatus(OFF);
	for(j = 0; j < 8; j++) {
		glcd_writeByte(PAGE0, INSTRUCTION, 0x40);
		glcd_writeByte(PAGE0, INSTRUCTION, 0xb8 + j);
		for(i = 0; i < 64; i++) {
			glcd_writeByte(PAGE0, DATA, imageArr[j][i]);
		}

		glcd_writeByte(PAGE1, INSTRUCTION, 0x40);
		glcd_writeByte(PAGE1, INSTRUCTION, 0xb8 + j);
		for(i = 0; i < 64; i++) {
			glcd_writeByte(PAGE1, DATA, imageArr[j][64+i]);
		}
	}
	glcd_displayStatus(ON);
}


void glcd_loadingBar(void) {
	uint8_t i;

	glcd_writeByte(PAGE0, INSTRUCTION, 0x40 + 18);
	glcd_writeByte(PAGE0, INSTRUCTION, 0xbe);
	for(i = 0; i < (64 - 18); i++) {
		glcd_writeByte(PAGE0, DATA, 0xfc);
		HAL_Delay(25);
	}

	glcd_writeByte(PAGE1, INSTRUCTION, 0x40);
	glcd_writeByte(PAGE1, INSTRUCTION, 0xbe);
	for(i = 0; i < (64 - 16); i++) {
		glcd_writeByte(PAGE1, DATA, 0xfc);
		HAL_Delay(25);
	}
}

/* USER CODE END 4 */

